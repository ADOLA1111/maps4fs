{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Linux / Mac Script",
            "type": "python",
            "request": "launch",
            "program": "maps4fs/ui.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
                "LOG_LEVEL": "DEBUG",
            }
        },
        {
            "name": "Streamlit: webui",
            "type": "process",
            "request": "launch",
            "preLaunchTask": "Run Streamlit",
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
                "LOG_LEVEL": "DEBUG",
            }
        },
        {
            "name": "Release Script",
            "type": "python",
            "request": "launch",
            "program": "release.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder};${PYTHONPATH}",
                "LOG_LEVEL": "DEBUG",
            }
        },

        {
            "name": "Linux / Mac  Bot",
            "type": "python",
            "request": "launch",
            "program": "bot/bot.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
                "LOG_LEVEL": "DEBUG",
            }
        }
    ]
}